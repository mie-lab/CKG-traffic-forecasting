{
  "rnn_units": 100,
  "lambda": 0.0015,
  "bidir_adj_mx": true,

  "scaler": "normal",
  "load_external": false,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": true,
  "add_day_in_week": false,

  "max_epoch": 500,
  "learner": "adam",
  "learning_rate": 0.001,
  "lr_decay": true,
  "lr_scheduler": "multisteplr",
  "lr_decay_ratio": 0.5,
  "steps": [150, 250, 350, 450],
  "clip_grad_norm": true,
  "max_grad_norm": 5,
  "use_early_stop": false,
  "patience": 50
}
