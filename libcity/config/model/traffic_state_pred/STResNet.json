{
  "nb_residual_unit": 12,
  "batch_norm": false,

  "scaler": "minmax11",
  "load_external": true,
  "normal_external": false,
  "ext_scaler": "none",
  "add_time_in_day": true,
  "add_day_in_week": false,
  "external_time": true,

  "max_epoch": 500,
  "learner": "adam",
  "learning_rate": 0.001,
  "lr_decay": true,
  "lr_scheduler": "multisteplr",
  "lr_decay_ratio": 0.5,
  "steps": [150, 250, 350, 450],
  "clip_grad_norm": true,
  "max_grad_norm": 5,
  "use_early_stop": false,
  "patience": 50
}
